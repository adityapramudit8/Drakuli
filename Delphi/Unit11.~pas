procedure TMengirimBarang.Button1Click(Sender: TObject);
begin
  DataModule1.ADOQuery19.Close();
  DataModule1.ADOQuery19.SQL.Clear();
  DataModule1.ADOQuery19.SQL.Add('select * From MENGIRIM_PRODUK ');

  DataModule1.ADOQuery19.SQL.Add('where 1=1');

  if (CheckBox1.Checked) then
  begin
    DataModule1.ADOQuery19.SQL.Add('and ID_JASA like :ID_JASA');
    DataModule1.ADOQuery19.Parameters.ParamByName('ID_JASA').Value := EDit1.Text;
  end;
  if (CheckBox2.Checked) then
  begin
    DataModule1.ADOQuery19.SQL.Add('and ID_PRODUK like :ID_PRODUK');
    DataModule1.ADOQuery19.Parameters.ParamByName('ID_PRODUK').Value := EDit2.Text;
  end;
  if (CheckBox3.Checked) then
  begin
    DataModule1.ADOQuery19.SQL.Add('and TANGGAL_PENGIRIMAN like :TANGGAL_PENGIRIMAN');
    DataModule1.ADOQuery19.Parameters.ParamByName('TANGGAL_PENGIRIMAN').Value := EDit3.Text;
  end;
  if (CheckBox4.Checked) then
  begin
    DataModule1.ADOQuery19.SQL.Add('and TANGGAL_SAMPAI like :TANGGAL_SAMPAI');
    DataModule1.ADOQuery19.Parameters.ParamByName('TANGGAL_SAMPAI').Value := EDit4.Text;
  end;
  if (CheckBox5.Checked) then
  begin
    DataModule1.ADOQuery19.SQL.Add('and LAMA_SAMPAI like :LAMA_SAMPAI');
    DataModule1.ADOQuery19.Parameters.ParamByName('LAMA_SAMPAI').Value := EDit5.Text;
  end;
  DataModule1.ADOQuery19.SQL.Add('order by ID_JASA asc');
  DataModule1.ADOQuery19.Open;
end;
procedure TMengirimBarang.Button2Click(Sender: TObject);
begin
  with DataModule1.ADOQuery20 do
  begin
    SQL.Clear();
    SQL.Add('insert into MENGIRIM_PRODUK(ID_JASA,ID_PRODUK,TANGGAL_PENGIRIMAN,TANGGAL_SAMPAI,LAMA_SAMPAI) values(:ID_JASA,:ID_PRODUK,:TANGGAL_PENGIRIMAN, :TANGGAL_SAMPAI, :LAMA_SAMPAI)');
    Parameters.ParamByName('ID_JASA').Value := Edit6.Text;
    Parameters.ParamByName('ID_PRODUK').Value := Edit7.Text;
    Parameters.ParamByName('TANGGAL_PENGIRIMAN').Value := Edit8.Text;
    Parameters.ParamByName('TANGGAL_SAMPAI').Value := Edit9.Text;
    Parameters.ParamByName('LAMA_SAMPAI').Value := Edit10.Text;
    ExecSQL();
end;
  DataModule1.ADOQuery19.Close();
  DataModule1.ADOQuery19.Open();
end;

procedure TMengirimBarang.Button3Click(Sender: TObject);
begin
  with DataModule1.ADOQuery20 do
  begin
    SQL.Clear();
    SQL.Add('delete from MENGIRIM_PRODUK where ID_JASA = :ID_JASA');
    Parameters.ParamByName('ID_JASA').Value := Edit6.Text;
    ExecSQL();
  end;
  DataModule1.ADOQuery19.Close();
  DataModule1.ADOQuery19.Open();
end;

procedure TMengirimBarang.Button4Click(Sender: TObject);
begin
with DataModule1.ADOQuery20 do
  begin
    SQL.Clear();
    SQL.Add('update MENGIRIM_PRODUK set ID_JASA=:ID_JASA, ID_PRODUK=:ID_PRODUK,TANGGAL_PENGIRIMAN=:TANGGAL_PENGIRIMAN,TANGGAL_SAMPAI=:TANGGAL_SAMPAI,LAMA_SAMPAI=:LAMA_SAMPAI');
    SQL.Add ('where ID_JASA = :ID_JASA');
    Parameters.ParamByName('ID_JASA').Value := Edit6.Text;
    Parameters.ParamByName('ID_PRODUK').Value := Edit7.Text;
    Parameters.ParamByName('TANGGAL_PENGIRIMAN').Value := Edit8.Text;
    Parameters.ParamByName('TANGGAL_SAMPAI').Value := Edit9.Text;
    Parameters.ParamByName('LAMA_SAMPAI').Value := Edit10.Text;
    ExecSQL();
  end;
  DataModule1.ADOQuery19.Close();
  DataModule1.ADOQuery19.Open();
end;


